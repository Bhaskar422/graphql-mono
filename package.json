{
  "name": "graphql-blog-monorepo",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "typescript": "^5.0.0",
    "tsx": "^3.0.0",
    "pnpm": "^8.0.0"
  },
  "scripts": {
    "install:all": "pnpm -w install",
    "typecheck": "pnpm -w -r run typecheck",
    "lint": "pnpm -w -r run lint",
    "dev:server": "pnpm --filter server dev",
    "dev:web": "pnpm --filter web dev",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:web\""
  }
}
